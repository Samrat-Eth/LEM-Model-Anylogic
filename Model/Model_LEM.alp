for(int i=0; i<100; i++)	
{
	for (int m=difference; m<1200; m++)
	{
		houses.get(i).bid_Propensity[m] = 0.0;
	}
}	//ends here


//MRE Algorithm for bids
for (int i=0; i<100; i++)
{
	for (int j=0; j<difference; j++)
	{
